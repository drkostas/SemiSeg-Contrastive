{
  "model": "DeepLab",
  "version": "3",
  "dataset": "minifrance_lbl",
  "city": "Nice",
  "training": {
    "batch_size": 5,
    "num_workers": 3,
    "optimizer": "SGD",
    "momentum": 0.9,
    "num_iterations": 150000,
    "learning_rate": 2e-4,
    "lr_schedule": "Poly",
    "pretraining": "COCO",
    "lr_schedule_power": 0.9,
    "weight_decay": 5e-4,
    "data": {
      "split_id_list": 0,
      "labeled_samples": 100,
      "input_size": "512,512"
    }
  },
  "seed": 5555,
  "ignore_label": -1,
  "utils": {
    "save_checkpoint_every": 200000,
    "checkpoint_dir": "/data/datasets/DFC2022/checkpoints/Deeplabv3",
    "val_per_iter": 1000,
    "save_best_model": true
  }
}
